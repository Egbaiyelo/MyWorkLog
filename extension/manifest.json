{
    "manifest_version": 3,
    "name": "MyWorkLog-dev-0.0",
    "version": "0.1.0",
    "description": "testing myWorkLog",
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.myworkdayjobs.com/*"
            ],
            "js": [
                "dist/content.bundle.js"
            ],
            "type": "module",
            "run_at": "document_idle"
        }
    ],
    "permissions": [
        "storage",
        "scripting",
        "activeTab",
        "tabs",
        "nativeMessaging",
        "identity",
        "identity.email"
    ],
    "host_permissions": [
        "*://*.myworkdayjobs.com/*"
    ],
    "action": {},
    "key": "",
    "oauth2": {
        "client_id": "343477439158-o44vju3bgtkjjq0sdu16nvl7kmde5shs.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/userinfo.email",
            "https://www.googleapis.com/auth/userinfo.profile"
        ]
    },
    "web_accessible_resources": [
        {
            "resources": [
                "icons/*",
                "pages/*",
                "files/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}